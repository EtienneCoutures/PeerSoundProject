{
  "name": "angular-electron",
  "version": "4.2.2",
  "description": "Angular 6 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --port 4200",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^7.2.0-beta.0",
    "@angular/cdk": "^7.0.0-rc.2",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/material": "^7.0.0-rc.2",
    "@mdi/font": "^2.7.94",
    "@progress/kendo-theme-default": "^2.56.0",
    "@types/socket.io-client": "^1.4.32",
    "bootstrap": "^4.1.3",
    "electron-online": "^1.0.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "ngx-perfect-scrollbar": "^6.3.1",
    "ngx-scrollbar": "^2.2.0",
    "node-id3": "^0.1.6",
    "plangular-3": "^3.1.2",
    "popper.js": "^1.14.4",
    "rxjs-compat": "^6.3.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "video.js": "^7.3.0",
    "videojs-youtube": "^2.6.0",
    "youtube-audio-stream": "^0.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "^7.1.0",
    "@angular/common": "7.2.0-beta.0",
    "@angular/compiler": "7.2.0-beta.0",
    "@angular/compiler-cli": "7.2.0-beta.0",
    "@angular/core": "7.2.0-beta.0",
    "@angular/forms": "7.2.0-beta.0",
    "@angular/http": "7.2.0-beta.0",
    "@angular/language-service": "7.2.0-beta.0",
    "@angular/platform-browser": "7.2.0-beta.0",
    "@angular/platform-browser-dynamic": "7.2.0-beta.0",
    "@angular/router": "^7.2.0-beta.0",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "electron": "^2.0.7",
    "electron-builder": "20.28.1",
    "electron-reload": "^1.2.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "^5.4.0",
    "rxjs": "6.3.3",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "3.1.6",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.26"
  }
}
